<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.StackPane?>

<StackPane xmlns:fx="http://javafx.com/fxml" fx:controller="edu.miracosta.cs112.finalproject.finalproject.EndViewController">
    <ImageView fx:id="beeImageView" fitHeight="200" fitWidth="200" />
    <TextField fx:id="beeInfoTextField" promptText="Bee stats will appear here..." editable="false" />
    <Button fx:id="restartButton" text="Restart Game" onAction="#restartGame" />
</StackPane>
